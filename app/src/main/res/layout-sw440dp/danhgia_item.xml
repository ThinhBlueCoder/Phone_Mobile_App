<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@color/white"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/anhdtbl"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:src="@drawable/logovip3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tendtbl"
        android:layout_width="270dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="54dp"
        android:layout_marginEnd="1dp"
        android:text="TextView"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/anhdtbl"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/nhapnoidung"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="20dp"
        android:gravity="top"
        android:hint="Nhập nhận xét"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/anhdtbl"
        />

    <ImageView
        android:id="@+id/sao1"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="12dp"
        android:src="@drawable/saodau"
        app:layout_constraintEnd_toStartOf="@+id/sao2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nhapnoidung"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/sao2"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="12dp"
        android:src="@drawable/saodau"
        app:layout_constraintEnd_toStartOf="@+id/sao3"
        app:layout_constraintStart_toEndOf="@+id/sao1"
        app:layout_constraintTop_toBottomOf="@+id/nhapnoidung"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/sao3"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="12dp"
        android:src="@drawable/saodau"
        app:layout_constraintEnd_toStartOf="@+id/sao4"
        app:layout_constraintStart_toEndOf="@+id/sao2"
        app:layout_constraintTop_toBottomOf="@+id/nhapnoidung"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/sao4"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="12dp"
        android:src="@drawable/saodau"
        app:layout_constraintEnd_toStartOf="@+id/sao5"
        app:layout_constraintStart_toEndOf="@+id/sao3"
        app:layout_constraintTop_toBottomOf="@+id/nhapnoidung"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/sao5"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="12dp"
        android:src="@drawable/saodau"
        app:layout_constraintStart_toEndOf="@+id/sao4"
        app:layout_constraintTop_toBottomOf="@+id/nhapnoidung"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/ketquasao"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="9dp"
        android:text="0"
        android:textSize="25sp"
        app:layout_constraintEnd_toStartOf="@+id/textView4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sao1"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="9dp"
        android:layout_marginTop="12dp"
        android:text="Sao"
        android:textSize="25sp"
        app:layout_constraintStart_toEndOf="@+id/ketquasao"
        app:layout_constraintTop_toBottomOf="@+id/sao1"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btnbinhluan"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        android:text="Bình Luận"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.976" />
</androidx.constraintlayout.widget.ConstraintLayout>